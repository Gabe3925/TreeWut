<% if @tree.errors.any? %>
<b>Errors:</b>
<ul>
  <% @tree.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
  <% end %>
</ul>
<% end %>



<div class="new_tree_form">
  <%= form_for [@tree] do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :species %>
    <%= f.text_field :species %><br>

    <%= f.label :latitude %> (XX.000000)
    <%= f.text_field :latitude %><br>

     <%= f.label :longitude %> (XX.000000)
    <%= f.text_field :longitude %><br>

    <%= f.label :description %>
    <%= f.text_field :description %><br>

    <%= f.label :height_in_feet %>
    <%= f.text_field :height %><br>

    <%= f.label :Enter_image_URL %>
    <%= f.text_field :image_data %><br>

    <%= f.submit "Submit", id: "submit" %>
  <% end %>
</div>
