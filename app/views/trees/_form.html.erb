<%= form_for [@tree] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :location_url %>
  <%= f.text_field :location_url %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :height %>
  <%= f.number_field :height %>

  <%= f.submit "Submit", id: "submit" %>
<% end %>
