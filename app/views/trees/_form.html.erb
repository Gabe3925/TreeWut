<%= form_for [@tree] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :species %>
  <%= f.text_field :species %>

  <%= f.label :latitude %>
  <%= f.text_field :latitude %>

   <%= f.label :longitude %>
  <%= f.text_field :longitude %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :height %>
  <%= f.text_field :height %>

  <%= f.submit "Submit", id: "submit" %>
<% end %>
